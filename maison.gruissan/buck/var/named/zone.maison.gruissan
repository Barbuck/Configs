; zone.maison.gruissan
$TTL	86400
$ORIGIN maison.gruissan.
@			1D IN SOA	buck.maison.gruissan. hostmaster.maison.gruissan. (
	  2014200401		; serial 
	  3H		        ; refresh
	  15M		        ; retry
	  1W		        ; expiry
  	1D )		      ; minimum

  IN NS	buck.maison.gruissan.
buck            IN A 192.168.3.1
eeepc           IN A 192.168.3.10
slackware.buck  IN A 192.168.3.1
freebsd.buck    IN A 192.168.3.1
